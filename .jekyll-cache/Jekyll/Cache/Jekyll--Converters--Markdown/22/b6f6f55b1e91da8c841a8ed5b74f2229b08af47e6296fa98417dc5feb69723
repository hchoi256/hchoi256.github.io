    print("Hello, World!")
"""</span><span class="p">,</span> <span class="n">language</span><span class="o">=</span><span class="s">"python"</span><span class="p">)</span>

<span class="n">st</span><span class="p">.</span><span class="n">latex</span><span class="p">(</span><span class="sa">r</span><span class="s">"""a + ar + a r^2 + a r^3 + \cdots +  a r^{n-1} = \sum_{k=0}^{n-1} ar^k = a \left(\frac{1-r^{n}}{1-r}\right)"""</span><span class="p">)</span>
</code></pre></div></div>

